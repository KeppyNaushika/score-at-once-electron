{
	"name": "score-at-once-electron",
	"description": "score-at-once-electron",
	"version": "0.1.0",
	"license": "MIT",
	"author": {
		"name": "keppy"
	},
	"repository": {
		"url": "https://github.com/keppy/score-at-once-electron.git"
	},
	"main": "dist/main.js",
	"scripts": {
		"dev": "rimraf dist && run-p dev:webpack dev:electron",
		"build": "rimraf dist && cross-env NODE_ENV=\"production\" webpack --progress",
		"dev:webpack": "cross-env NODE_ENV=\"development\" webpack --progress",
		"dev:electron": "wait-on dist/main.js dist/index.html && electronmon ."
	},
	"electronmon": {
		"patterns": [
			"dist/**/*"
		]
	},
	"dependencies": {
		"material-symbols": "^0.5.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-router-dom": "^6.9.0"
	},
	"devDependencies": {
		"@svgr/webpack": "^6.5.1",
		"@types/node": "^18.14.6",
		"@types/react": "^18.0.28",
		"@types/react-dom": "^18.0.11",
		"cross-env": "^7.0.3",
		"css-loader": "^6.7.3",
		"electron": "^23.1.2",
		"electronmon": "^2.0.2",
		"html-webpack-plugin": "^5.5.0",
		"license-checker": "^25.0.1",
		"mini-css-extract-plugin": "^2.7.2",
		"npm-run-all": "^4.1.5",
		"rimraf": "^4.3.0",
		"ts-loader": "^9.4.2",
		"ts-node": "^10.9.1",
		"typescript": "^4.9.5",
		"wait-on": "^7.0.1",
		"webpack": "^5.75.0",
		"webpack-cli": "^5.0.1"
	}
}
